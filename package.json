{
  "name": "chromatography",
  "version": "2.0.2",
  "description": "Tools for storing, search and analize GC/MS spectra",
  "main": "src/index.js",
  "scripts": {
    "build": "cheminfo build --no-uglify --root Chromatography",
    "build-doc": "cheminfo doc",
    "publish-doc": "cheminfo doc --publish",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "jest",
    "watch": "npm run test -- --watch",
    "test-travis": "npm run test-ava && npm run eslint && npm run report-coverage",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo-js/chromatography.git"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/data/.*"
    ]
  },
  "author": "MichaÃ«l Zasso",
  "contributors": [
    "Miguel Asencio <maasencioh@gmail.com> (https://github.com/maasencioh)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo-js/chromatography/issues"
  },
  "homepage": "https://github.com/cheminfo-js/chromatography#readme",
  "devDependencies": {
    "bluebird": "^3.3.4",
    "cheminfo-tools": "^1.9.0",
    "codecov": "^1.0.1",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.4.0",
    "eslint-config-cheminfo": "^1.2.0",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "jest": "^19.0.2",
    "nyc": "^8.3.0",
    "should": "^11.2.1"
  },
  "dependencies": {
    "binary-search": "^1.3.2",
    "chemcalc": "^3.3.2",
    "jcampconverter": "^2.3.0",
    "ml-gsd": "^2.0.1",
    "ml-regression": "^4.1.1",
    "xy-parser": "^1.2.0"
  }
}
