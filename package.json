{
  "name": "chromatography",
  "version": "4.1.3",
  "description": "Tools for storing, searching and analyzing GC/MS data",
  "main": "lib/index.js",
  "module": "src/index.js",
  "types": "chromatography.d.ts",
  "sideEffects": false,
  "cheminfo": {
    "build": {
      "namedExports": {
        "utf8": [
          "decode",
          "encode"
        ]
      }
    }
  },
  "files": [
    "chromatography.d.ts",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root Chromatography",
    "build-docs": "typedoc --out docs --name \"Chromatography\" --mode file --includeDeclarations --excludeExternals --hideGenerator --excludePrivate --moduleResolution node --target ESNext chromatography.d.ts",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "rollup -c",
    "test": "npm run test-coverage && npm run eslint",
    "test-only": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/chromatography.git"
  },
  "jest": {
    "testEnvironment": "node",
    "transformIgnorePatterns": []
  },
  "author": "MichaÃ«l Zasso",
  "contributors": [
    "Miguel Asencio <maasencioh@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/chromatography/issues"
  },
  "homepage": "https://github.com/cheminfo/chromatography#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@types/jest": "^26.0.14",
    "cheminfo-build": "^1.1.5",
    "eslint": "^7.10.0",
    "eslint-config-cheminfo": "^5.2.0",
    "esm": "^3.2.25",
    "jest": "^26.4.2",
    "jest-matcher-deep-close-to": "^2.0.1",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "spectrum-generator": "^4.0.2",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "binary-search": "^1.3.6",
    "is-any-array": "0.1.0",
    "isotopic-distribution": "^1.1.9",
    "jcampconverter": "^7.3.1",
    "mf-parser": "^1.1.6",
    "ml-array-max": "^1.1.2",
    "ml-array-mean": "^1.1.3",
    "ml-array-median": "^1.1.3",
    "ml-array-sum": "^1.1.3",
    "ml-gsd": "^5.0.1",
    "ml-matrix": "^6.5.1",
    "ml-ngmca": "^0.0.2",
    "ml-pca": "^4.0.1",
    "ml-regression-polynomial": "^2.1.0",
    "ml-spectra-processing": "^4.2.1",
    "mzdata": "^2.1.0",
    "netcdf-gcms": "^1.4.0",
    "xy-parser": "^3.0.0"
  }
}
